{
  "name": "CheckPwd-Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Md5Pwd",
  "content_scripts":[
    {
     "matches" : [    
       "http://www.hi-pda.com/*",
       "http://www.md5.cc/*"
